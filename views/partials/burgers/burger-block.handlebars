<li class="burgerlist flow-text">
	{{dataValues.burger_name}}
	{{#unless dataValues.devoured}}
	<br>
	<button class="devbutton red waves-effect waves-light btn left-align" data-id="{{dataValues.id}}">Eat It</button>
	{{/unless}}
	{{#if dataValues.devoured}}
	<br>
	<button class="delbutton red waves-effect waves-light btn left-align" data-id="{{dataValues.id}}">Delete It</button>
	{{/if}}
</li>